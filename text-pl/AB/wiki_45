public static double ComputeArea_Version4(object shape)
 switch (shape)
 case Square s when s.Side == 0:
 case Circle c when c.Radius == 0:
 case Triangle t when t.Base == 0 || t.Height == 0:
 case Rectangle r when r.Length == 0 || r.Height == 0:
 return 0;
