var zdanie = new string[]
 // indeks od poczatku indeks od końca
 "Ala", // 0 ^7
 "ma", // 1 ^6
 "kota", // 2 ^5
 "a", // 3 ^4
 "kot", // 4 ^3
 "ma", // 5 ^2
 "Alę.", // 6 ^1
}; // 7 (lub words.Length) ^0
